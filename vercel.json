{
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/analyze",
            "destination": "/api.py"
        },
        {
            "source": "/health",
            "destination": "/api.py"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/dist/$1"
        }
    ]
}